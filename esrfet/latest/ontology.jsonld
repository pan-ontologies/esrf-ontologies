}, {
  "@id" : "http://purl.org/pan-science/ESRFET#3DXRD",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "3DXRD is a far field diffraction technique capable of reconstructing polycrystalline materials from the individual diffraction spots recorded in the diffraction pattern given that the crystallographic parameters of the material are known. The success of the technique is based on the ability of the individual diffraction spots to be separated. This means there is a trade off between the number of grains and the degree of strain that is measurable."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "3DXRD"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid590"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "3D X-ray Diffraction"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ARPES",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Angle-Resolved Photoemission Spectroscopy (ARPES) measures the energy and momentum of electrons emitted from a sample when illuminated by photons, mapping the electronic band structure and Fermi surface of materials."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ARPES"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Angle-Resolved Photoemission Spectroscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#BCDI",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Bragg Coherent Diffraction Imaging (BCDI) measures the coherent diffraction around a Bragg peak from a crystalline sample to reconstruct the 3D shape and internal strain field of individual nanocrystals. The result is a high-resolution image of the crystalline structure and deformation."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "BCDI"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Bragg Coherent Diffraction Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#BioSAXS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "BioSAXS is SAXS applied to biological macromolecules in solution to determine their size, shape, flexibility, and aggregation state in near-native conditions."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "BioSAXS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid1"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#CDI",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Coherent Diffraction Imaging (CDI) measures the far-field diffraction pattern from a sample illuminated with a coherent X-ray beam. The image of the sample is reconstructed computationally from the diffraction data using phase retrieval algorithms, yielding high-resolution, lensless imaging of the electron density or scattering potential."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "CDI"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "CXD"
  }, {
    "@value" : "CXDI"
  }, {
    "@value" : "Coherent X-ray Diffraction"
  }, {
    "@value" : "Coherent X-ray Diffraction Imaging"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Coherent Diffraction Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#CMS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Conventional Mssbauer Spectroscopy measures the resonant absorption of X-rays by Mssbauer-active nuclei (e.g. Fe-57) using a radioactive source scanned in velocity. It provides information on oxidation state, magnetic ordering, and local electronic and structural environment through hyperfine interactions."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "CMS"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Conventional Mssbauer Spectroscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#CRYO-EM",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Cryogenic Electron Microscopy (Cryo-EM) is EM performed on rapidly frozen samples to preserve native structures in a near-physiological, vitrified state. It enables high-resolution 3D reconstruction of biological macromolecules and complexes without the need for crystallization."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "CRYO-EM"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid11"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "Cryogenic electron microscopy"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Cryo-electron microscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#CXDI",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Coherent X-ray Diffraction Imaging (CXDI) measures the coherent X-ray diffraction pattern from an isolated object or nanostructure. The 3D electron density is reconstructed computationally using phase retrieval, providing high-resolution, lensless imaging of the sample's internal structure."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "CXDI"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Coherent X-ray Diffraction Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#DAC",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Diamond Anvil Cell to apply pressure on the sample"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#sample_input"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#DAFS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Diffraction Anomalous Fine Structure (DAFS) measures the intensity of Bragg diffraction as a function of X-ray photon energy near an absorption edge. It combines diffraction and XAS to provide element-specific structural information, such as site occupancy, bonding environment, and local distortions in crystals."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "DAFS"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Diffraction Anomalous Fine Structure"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#DCT",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "DCT is a near-field diffraction-based imaging technique that provides high-resolution grain maps of polycrystalline materials (Ludwig et al. 2008). For each individual grain, the technique can provide orientation and average elastic strain tensor components with an accuracy of a few times 1e-4 (Reischig et al. 2013). The technique combines the concepts of image reconstruction from projections (tomography) and X-ray diffraction imaging (topography)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "DCT"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid19"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Diffraction Contrast Tomography"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#DFXM",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Dark-Field X-ray Microscopy (DFXM) measures X-ray diffraction in a dark-field imaging geometry to map crystalline structure, orientation, strain, and defects in 3D within bulk materials. It provides spatially resolved imaging of internal microstructures at micrometer to sub-micrometer resolution."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "DFXM"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Dark-Field X-ray Microscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ED-EXAFS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Energy Dispersive Extended X-ray Absorption Fine Structure (ED-EXAFS) measures oscillations in X-ray absorption probability beyond the edge with energy-dispersive detection, providing local atomic structural information such as bond distances and coordination numbers, acquired quickly without angular scanning."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ED-EXAFS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid32"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive Extended X-ray Absorption Fine Structure"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ED-XANES",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Energy Dispersive X-ray Absorption Near Edge Structure (ED-XANES) measures the X-ray absorption probability near the absorption edge using energy-dispersive detection, providing rapid information about the oxidation state and electronic structure without scanning incident energy."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ED-XANES"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid44"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive X-ray Absorption Near Edge Structure"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ED-XAS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Energy Dispersive X-ray Absorption Spectroscopy (ED-XAS) measures X-ray absorption as a function of energy simultaneously by dispersing the X-ray beam in space with a polychromatic source and position-sensitive detector, enabling rapid acquisition of absorption spectra without scanning the incident energy."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ED-XAS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid56"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive X-ray Absorption Spectroscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ED-XRD",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Energy Dispersive X-ray Diffraction (ED-XRD) measures diffraction patterns of a sample by detecting diffracted X-rays at a fixed angle while dispersing their energies, enabling fast phase identification and structural analysis without angular scanning."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ED-XRD"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid68"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive X-ray Diffraction"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ED-XRF",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Energy Dispersive X-ray Fluorescence Spectroscopy (ED-XRF) measures the energies of fluorescent X-rays emitted by a sample excited by a broad-spectrum X-ray source, providing elemental composition analysis without scanning the incident energy."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ED-XRF"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid78"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive X-ray Fluorescence Spectroscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ED-XRPD",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Energy Dispersive X-ray Powder Diffraction (ED-XRPD) measures diffraction patterns of powdered samples by detecting diffracted X-rays at a fixed angle while varying their energy, enabling rapid phase identification and structural analysis without scanning angles."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ED-XRPD"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid90"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive X-ray Powder Diffraction"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#EDS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "EDS measures X-ray photons coming from a sample during electron irradition  with an energy dispersive detector and derives elemental composition from the energy of the fluorescence photons"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "EDS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid102"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Energy Dispersive X-ray Spectroscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#EM",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Electron Microscopy (EM) uses a beam of electrons as illumination, controlled by electron optics analogous to glass lenses in light microscopes, to measure electron scattering or transmission through a sample. This technique provides high-resolution images and electron diffraction patterns, revealing structural, morphological, and compositional information at nanometer to atomic scales."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "EM"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://purl.org/pan-science/ESRFET#experimental_technique"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid114"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Electron Microscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#ESRF",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://xmlns.com/foaf/0.1/Organization"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@id" : "https://ror.org/02550n020"
  } ],
  "http://xmlns.com/foaf/0.1/homepage" : [ {
    "@id" : "http://www.esrf.fr"
  } ],
  "http://xmlns.com/foaf/0.1/name" : [ {
    "@value" : "ESRF"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#EXAFS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Extended X-ray Absorption Fine Structure (EXAFS) measures oscillations in X-ray absorption above an elements absorption edge, providing information on the local atomic environment, including distances, coordination numbers, and disorder around the absorbing atoms."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "EXAFS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid118"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Extended X-ray Absorption Fine Structure"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FF-EXAFS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FF-EXAFS measures the extended region of the absorption spectrum across a 2D sample area using broad X-ray beam and an area detector, providing spatially resolved information on local atomic structure, such as bond lengths and coordination."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FF-EXAFS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid128"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Full-Field Extended X-ray Absorption Fine Structure Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FF-XANES",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FF-XANES measures the near-edge region of the absorption spectrum across a 2D sample area using broad X-ray beam and an area detector, providing spatially resolved information on chemical state and electronic structure."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FF-XANES"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid140"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "FullField X-ray Absorption Near Edge Structure Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FF-XAS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FF-XAS measures X-ray absorption spectra across a 2D sample area using broad X-ray beam and an area detector, providing spatially resolved insight into electronic and atomic structure."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FF-XAS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid152"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Full-Field X-ray Absorption Spectroscopy Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FF-XRF",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FF-XRF measures X-ray fluorescence spectra across a 2D sample area using broad X-ray beam and an area detector, providing 2D elemental distribution maps in projection."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FF-XRF"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid164"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Full-Field X-ray Fluorescence Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FS-EXAFS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FS-EXAFS measures the extended region of the absorption spectrum across a 2D sample area using a focused X-ray beam, providing spatially resolved information on local atomic structure, such as bond lengths and coordination."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FS-EXAFS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid178"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Full-Spectral Extended X-ray Absorption Fine Structure Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FS-XAS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FS-XAS measures X-ray absorption spectra across a 2D sample area using a focused X-ray beam, providing spatially resolved insight into electronic and atomic structure."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FS-XAS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid190"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Full-Spectral X-ray Absorption Spectroscopy Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FSXANES",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "FS-XANES measures the near-edge region of the absorption spectrum across a 2D sample area using a focused X-ray beam, providing spatially resolved information on chemical state and electronic structure."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FS-XANES"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid600"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "FullSpectral X-ray Absorption Near Edge Structure Imaging"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#FTIR",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Fourier Transform Infrared Spectroscopy (FTIR) measures the absorption of infrared light by a sample as a function of wavelength, providing information on molecular vibrations and chemical bonds to identify and characterize materials."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FTIR"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid202"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Fourier Transform Infrared Spectroscopy"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#GISAXS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Grazing-Incidence Small-Angle X-ray Scattering (GISAXS) measures scattered X-rays at small angles from a sample surface under shallow incidence, providing nanoscale information about thin film and surface morphology, including size, shape, and arrangement of nanostructures."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "GISAXS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid208"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Grazing Incidence Small Angle X-ray Scattering"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#GIWAXS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Grazing-Incidence Wide-Angle X-ray Scattering (GIWAXS) measures scattered X-rays at wide angles under shallow incidence, providing information on molecular packing, crystalline order, and orientation in thin films and surfaces."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "GIWAXS"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid218"
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "Grazing Incidence Wide Angle X-ray Scattering"
  } ]
}, {
  "@id" : "http://purl.org/pan-science/ESRFET#GIXRD",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Grazing-Incidence X-ray Diffraction (GIXRD) measures diffraction patterns from a sample surface using X-rays at shallow incident angles, providing structural information on thin films, surfaces, and near-surface regions, including phase composition and strain."
  } ],
