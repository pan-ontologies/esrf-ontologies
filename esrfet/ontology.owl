                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Wide-Angle X-ray Scattering (WAXS) measures X-ray scattering at larger angles than SAXS to provide information on short-range order, crystalline structure, and molecular packing in materials.</rdfs:comment>
        <rdfs:label>WAXS</rdfs:label>
        <skos:prefLabel>Wide-angle X-ray Scattering</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#WDS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#WDS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_count"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_energy"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
