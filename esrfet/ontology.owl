    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#high_temperature -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#high_temperature">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_temperature"/>
