        <rdfs:comment>Liquid He (4.2 K), superconductivity, quantum devices</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#technique_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result has properties which are being detected.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detected_photon_property -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detected_photon_property">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result being photons which properties are being measured.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#space_time"/>
        <rdfs:comment>Detect as a function of the incident photon or particle energy</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detection_as_a_function_of_limited_number_of_incident_energies -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detection_as_a_function_of_limited_number_of_incident_energies">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detection_resolution -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detection_resolution">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the resolution of the certain result properties are measured.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detector_type -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detector_type">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#technique_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result has properties which are being detected with a detector that has certain characteristics.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#diffraction -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#diffraction">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
        <rdfs:comment>Detecting interference of elastically scattering photons or particles</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#diffraction"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#disperse_incident_energy_in_space -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#disperse_incident_energy_in_space">
