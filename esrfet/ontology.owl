        <rdfs:comment>Used in fundamental physics, surface science</rdfs:comment>
        <skos:altLabel>XHV</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#extreme_pressure -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#extreme_pressure">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_pressure"/>
        <rdfs:comment>&gt; 1 × 10¹¹ Pa</rdfs:comment>
        <rdfs:comment>Planetary interiors, shock compression studies</rdfs:comment>
        <skos:altLabel>EP</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#extreme_temperature -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#extreme_temperature">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_temperature"/>
        <rdfs:comment>5,000 – 10⁷ K</rdfs:comment>
        <rdfs:comment>Fusion plasmas, stellar photospheres, inertial confinement</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#fluorescence -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#fluorescence">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
        <rdfs:comment>Detecting one result after atomic core excitation (fluorescence)</rdfs:comment>
    </owl:Class>
    


