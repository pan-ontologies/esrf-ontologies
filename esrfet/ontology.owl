        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>Ultra-Small Angle X-ray Photon Correlation Spectroscopy (USA-XPCS) measures time-dependent fluctuations of coherent X-ray scattering at ultra-small angles to probe slow dynamics and structural rearrangements on large length scales in complex materials.</rdfs:comment>
        <rdfs:label>USA-XPCS</rdfs:label>
        <skos:prefLabel>Ultra-Small Angle X-ray Photon Correlation Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#USAXS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#USAXS">
        <owl:equivalentClass>
