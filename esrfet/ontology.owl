        <rdfs:comment>Metallurgy, refractory studies</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#high_vacuum -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#high_vacuum">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_pressure"/>
        <rdfs:comment>1 × 10⁻⁷ – 1 × 10⁻³ Pa</rdfs:comment>
        <rdfs:comment>Thin film deposition, mass spectrometry</rdfs:comment>
        <skos:altLabel>HV</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#horizontal_linear_polarization -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#horizontal_linear_polarization">
