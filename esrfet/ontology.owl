        </owl:equivalentClass>
        <rdfs:comment>Scanning X-ray Fluorescence (S-XRF) measures elemental composition by scanning a focused X-ray beam across a sample and detecting emitted fluorescence, producing high-resolution 2D maps of elemental distributions in the sample.</rdfs:comment>
        <rdfs:label>S-XRF</rdfs:label>
        <skos:prefLabel>Scanning X-ray Fluorescence</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#SAD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#SAD">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>Single-wavelength Anomalous Diffraction (SAD) measures diffraction at one specific X-ray wavelength near an absorption edge to exploit anomalous scattering for phase determination in crystallography.</rdfs:comment>
        <rdfs:label>SAD</rdfs:label>
        <skos:prefLabel>Single-wavelength Anomalous Diffraction</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#SAXS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#SAXS">
        <owl:equivalentClass>
