        <skos:altLabel>SC-XRD</skos:altLabel>
        <skos:altLabel>SCD</skos:altLabel>
        <skos:altLabel>SXRD</skos:altLabel>
        <skos:prefLabel>Single Crystal X-ray Diffraction</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#SIXES -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#SIXES">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
