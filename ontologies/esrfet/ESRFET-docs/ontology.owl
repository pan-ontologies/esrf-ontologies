                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_energy"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#fluorescence"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#sample_incident_angle_smaller_than_critical_angle"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Total Reflection X-ray Fluorescence (TXRF) measures characteristic X-rays emitted by a sample under total external reflection conditions, providing highly sensitive, surface-specific elemental analysis with minimal sample preparation.</rdfs:comment>
        <rdfs:label>TXRF</rdfs:label>
        <skos:prefLabel>Total Reflection X-ray Fluorescence</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#USA-XPCS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#USA-XPCS">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>Ultra-Small Angle X-Ray Photon Correlation Spectroscopy (USA-XPCS) measures time-dependent fluctuations of coherent X-ray scattering at ultra-small angles to probe slow dynamics and structural rearrangements on large length scales in complex materials.</rdfs:comment>
        <rdfs:label>USA-XPCS</rdfs:label>
        <skos:prefLabel>Ultra-Small Angle X-Ray Photon Correlation Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#USAXS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#USAXS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#ultra_large_length_scales"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Ultra-Small Angle X-ray Scattering (USAXS) measures X-ray scattering at extremely small angles to probe structural features on length scales larger than those accessible by SAXS, such as large-scale aggregates, pores, or hierarchical structures.</rdfs:comment>
        <rdfs:label>USAXS</rdfs:label>
        <skos:prefLabel>Ultra-Small Angle X-ray Scattering</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#UV -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#UV">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#named_photon_energy_ranges"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#VLM -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#VLM">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>Visible Light Microscopy (VLM) captures images of a sample using visible light illumination and optical lenses, providing spatially resolved information about surface morphology, color, and structure at micrometer-scale resolution.</rdfs:comment>
        <rdfs:label>VLM</rdfs:label>
        <skos:prefLabel>Visible Light Microscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#WAXS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#WAXS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#small_length_scales"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Wide-Angle X-ray Scattering (WAXS) measures X-ray scattering at larger angles than SAXS to provide information on short-range order, crystalline structure, and molecular packing in materials.</rdfs:comment>
        <rdfs:label>WAXS</rdfs:label>
        <skos:prefLabel>Wide-angle X-ray Scattering</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#WDS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#WDS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_count"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_energy"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#fluorescence"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_with_a_detector_type"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#wavelength_dispersive_detector"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>WDS measures X-ray photons coming from a sample during irradition with a wavelength dispersive detector and derives elemental composition from the energy of the fluorescence photons</rdfs:comment>
        <rdfs:label>WDS</rdfs:label>
        <skos:prefLabel>Wavelength Dispersive X-ray Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XANES -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XANES">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_as_a_function_of"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#near_edge"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_count"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#atomic_core_excitation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>XANES (X-ray Absorption Near Edge Structure) measures the fine structure in X-ray absorption near an element’s absorption edge, providing information about the oxidation state, electronic structure, and coordination geometry of the absorbing atoms.</rdfs:comment>
        <rdfs:label>XANES</rdfs:label>
        <skos:prefLabel>X-ray Absorption Near Edge Structure</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XAS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XAS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_as_a_function_of"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_count"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#atomic_core_excitation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>XAS measures a signal related to the X-ray absorption probability—such as fluorescence, photon electrons, Auger electrons, or transmission—as a function of the incident X-ray photon energy. It probes atomic core-level excitations by removing core-shell electrons, providing element-specific information about the local chemical environment, oxidation state, and atomic structure around the absorbing atoms.</rdfs:comment>
        <rdfs:label>XAS</rdfs:label>
        <skos:prefLabel>X-ray Absorption Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XBH -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XBH">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Bragg Holography (XBH) measures interference patterns formed by Bragg-diffracted and reference X-rays from a crystal to reconstruct 3D atomic-scale images of the crystal structure.</rdfs:comment>
        <rdfs:label>XBH</rdfs:label>
        <skos:prefLabel>X-ray Bragg Holography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XCL -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XCL">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Computed Laminography (XCL) measures X-ray transmission through flat, extended samples at multiple tilted angles to reconstruct high-resolution 3D images of internal structures that are challenging for conventional tomography.</rdfs:comment>
        <rdfs:label>XCL</rdfs:label>
        <skos:prefLabel>X-ray Computed Laminography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XDMR -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XDMR">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Detected Magnetic Resonance (XDMR) measures changes in X-ray absorption or emission induced by magnetic resonance excitation, providing element-specific information on spin dynamics and magnetic excitations.</rdfs:comment>
        <rdfs:label>XDMR</rdfs:label>
        <skos:prefLabel>X-ray Detected Magnetic Resonance</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XDS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XDS">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-Ray Diffuse Scattering (XDS) measures scattered X-rays away from Bragg peaks to probe local disorder, defects, thermal vibrations, and nanoscale structural fluctuations in materials.</rdfs:comment>
        <rdfs:label>XDS</rdfs:label>
        <skos:prefLabel>X-Ray Diffuse Scattering</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XEOL -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XEOL">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Excited Optical Luminescence (XEOL) measures visible or near-visible light emitted from a sample after X-ray excitation, providing information on electronic structure, defect states, and luminescent centers.</rdfs:comment>
        <rdfs:label>XEOL</rdfs:label>
        <skos:prefLabel>X-ray Excited Optical Luminescence</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XES -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XES">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Emission Spectroscopy (XES) measures emitted X-ray photons from electronic transitions after X-ray excitation, revealing detailed information about the electronic structure and chemical state of elements. Unlike XRF, which identifies elemental composition through broad characteristic fluorescence lines, XES uses high energy resolution to resolve fine spectral features—such as valence-to-core transitions—providing deeper insight into the local chemical bonding and electronic environment.</rdfs:comment>
        <rdfs:label>XES</rdfs:label>
        <skos:prefLabel>X-ray Emission Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XFCT -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XFCT">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_as_a_function_of"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#tomography"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_count"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_energy"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#fluorescence"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>X-ray Fluorescence Tomography (XFCT) measures fluorescence X-rays emitted from a sample at multiple angles to reconstruct 3D elemental distributions with high sensitivity and spatial resolution.</rdfs:comment>
        <rdfs:label>XFCT</rdfs:label>
        <skos:prefLabel>X-ray Fluorescence Tomography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XFH -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XFH">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Fluorescence Holography (XFH) measures interference between fluorescent X-rays emitted from atoms and scattered reference waves to obtain 3D atomic-scale images of the local atomic arrangement around specific elements.</rdfs:comment>
        <rdfs:label>XFH</rdfs:label>
        <skos:prefLabel>X-ray Fluorescence Holography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XHT -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XHT">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray HoloTomography (XHT) combines holographic imaging and tomography by recording holograms at multiple angles to reconstruct high-resolution 3D electron density maps of a sample.</rdfs:comment>
        <rdfs:label>XHT</rdfs:label>
        <skos:prefLabel>X-ray HoloTomography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XLD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XLD">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Linear Dichroism (XLD) measures the difference in X-ray absorption for linearly polarized light oriented along different crystallographic axes, providing information on electronic anisotropy and orbital ordering.</rdfs:comment>
        <rdfs:label>XLD</rdfs:label>
        <skos:prefLabel>X-Ray Linear Dichroism</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XMCD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XMCD">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Magnetic Circular Dichroism (XMCD) measures the difference in absorption of left- and right-circularly polarized X-rays in a magnetized sample, probing element-specific magnetic moments and spin states.</rdfs:comment>
        <rdfs:label>XMCD</rdfs:label>
        <skos:prefLabel>X-ray Magnetic Circular Dichroism</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XMLD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XMLD">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Magnetic Linear Dichroism (XMLD) measures differences in X-ray absorption of linearly polarized light depending on magnetic order, sensitive to antiferromagnetic and magnetic domain structures.</rdfs:comment>
        <rdfs:label>XMLD</rdfs:label>
        <skos:prefLabel>X-ray Magnetic Linear Dichroism</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XMχD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XMχD">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray MagnetoChiral Dichroism (XMχD) measures differences in X-ray absorption related to the combined effects of magnetism and chirality in materials, revealing chiral magnetic structures.</rdfs:comment>
        <rdfs:label>XMχD</rdfs:label>
        <skos:prefLabel>X-ray MagnetoChiral Dichroism</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XNCD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XNCD">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Natural Circular Dichroism (XNCD) measures differences in absorption of circularly polarized X-rays in non-magnetic chiral materials, providing information on structural chirality and electronic asymmetry.</rdfs:comment>
        <rdfs:label>XNCD</rdfs:label>
        <skos:prefLabel>X-ray Natural Circular Dichroism</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XNHT -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XNHT">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Nano-HoloTomography (XNHT) is a high-resolution variant of XHT, providing nanoscale 3D imaging of internal sample structure by combining holography and tomography with nanoscale focusing.</rdfs:comment>
        <rdfs:label>XNHT</rdfs:label>
        <skos:prefLabel>X-ray Nano-HoloTomography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XPCI -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XPCI">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Phase-Contrast Imaging (XPCI) measures phase shifts of X-rays transmitted through a sample to enhance image contrast, enabling visualization of structures with low absorption contrast such as soft tissues or light materials.</rdfs:comment>
        <rdfs:label>XPCI</rdfs:label>
        <skos:altLabel>PCXI</skos:altLabel>
        <skos:altLabel>Phase-Contrast X-ray Imaging</skos:altLabel>
        <skos:prefLabel>X-ray Phase-Contrast Imaging</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XPCS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XPCS">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-Ray Photon Correlation Spectroscopy (XPCS) measures time fluctuations of coherent X-ray scattering intensity to probe nanoscale dynamics and temporal correlations in materials such as particle motion, phase transitions, or relaxation processes.</rdfs:comment>
        <rdfs:label>XPCS</rdfs:label>
        <skos:prefLabel>X-Ray Photon Correlation Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XPCT -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XPCT">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Phase-Contrast Tomography (XPCT) measures phase shifts of X-rays passing through a sample at multiple angles to reconstruct 3D images with enhanced contrast for weakly absorbing materials, revealing internal structures not visible in conventional absorption tomography.</rdfs:comment>
        <rdfs:label>XPCT</rdfs:label>
        <skos:prefLabel>Phase-Contrast Tomography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XPS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XPS">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Photoelectron Spectroscopy (XPS) measures the kinetic energy of electrons ejected by X-rays to determine elemental composition and chemical states near the surface of a material.</rdfs:comment>
        <rdfs:label>XPS</rdfs:label>
        <skos:prefLabel>X-ray Photoelectron Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRAYS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRAYS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>Experimental techniques based on irradiating a sample with X-rays to probe its properties by measuring interactions such as absorption, scattering, fluorescence, or diffraction, providing insights into structural, elemental, or electronic characteristics depending on the specific method used.</rdfs:comment>
        <rdfs:label>XRAYS</rdfs:label>
        <skos:prefLabel>X-ray Radiation Technique</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRCT -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRCT">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-Ray Computed Tomography (XRCT) measures X-ray absorption (transmission) of a sample from multiple angles and reconstructs a 3D volume, providing detailed internal structural information such as density variations, morphology, and defects without destroying the sample.</rdfs:comment>
        <rdfs:label>XRCT</rdfs:label>
        <skos:prefLabel>X-Ray Computed Tomography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>X-ray Diffraction (XRD) measures the diffraction pattern produced when X-rays interact with a crystalline sample. The pattern provides information about the crystal structure, phase identification, lattice parameters, and crystallite size of the material.</rdfs:comment>
        <rdfs:label>XRD</rdfs:label>
        <skos:prefLabel>X-ray diffraction</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRF -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRF">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_count"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_interaction_result_property"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#photon_energy"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#fluorescence"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>X-ray Fluorescence (XRF) measures characteristic X-ray photons emitted by a sample when it is irradiated with X-rays. The energy of the emitted fluorescence photons reveals the elemental composition and chemical state of the sample.</rdfs:comment>
        <rdfs:label>XRF</rdfs:label>
        <skos:prefLabel>X-ray Fluorescence Spectroscopy</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRPD -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRPD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_type"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#powder_sample"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Powder Diffraction (XRPD) measures diffraction patterns from polycrystalline or powdered samples to identify phases, determine crystal structure, lattice parameters, and quantify phase composition.</rdfs:comment>
        <rdfs:label>XRPD</rdfs:label>
        <skos:prefLabel>X-ray Powder Diffraction</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRR -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRR">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#measures_result_of_interaction"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#reflection"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/pan-science/ESRFET#requires_sample_input"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/pan-science/ESRFET#x-ray"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>X-ray Reflectometry (XRR) measures the intensity of X-rays reflected from thin films or multilayers as a function of incident angle to determine film thickness, density, and interface roughness.</rdfs:comment>
        <rdfs:label>XRR</rdfs:label>
        <skos:altLabel>X-ray reflectivity</skos:altLabel>
        <skos:altLabel>X-ray specular reflectivity</skos:altLabel>
        <skos:prefLabel>X-ray Reflectometry</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRS -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRS">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-ray Raman Scattering (XRS) measures inelastic scattering of hard X-rays by core electrons, enabling the study of low-Z elements’ electronic structure and bonding in bulk samples without the need for soft X-rays or vacuum conditions.</rdfs:comment>
        <rdfs:label>XRS</rdfs:label>
        <skos:prefLabel>X-ray Raman Scattering</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#XRT -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#XRT">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#experimental_technique"/>
        <rdfs:comment>X-Ray Diffraction Topography (XRT) records spatial variations in the intensity of X-ray diffraction from a crystal to image defects, dislocations, and strain fields with high spatial resolution, revealing crystal quality and internal microstructure.</rdfs:comment>
        <rdfs:label>XRT</rdfs:label>
        <skos:altLabel>X-Ray Diffraction Topography</skos:altLabel>
        <skos:altLabel>XDT</skos:altLabel>
        <skos:prefLabel>X-Ray Topography</skos:prefLabel>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#absorption -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#absorption">
        <owl:equivalentClass rdf:resource="http://purl.org/pan-science/ESRFET#atomic_core_excitation"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#area_detector -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#area_detector">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detector_type"/>
        <rdfs:comment>Detector that distinguishes the position of the waves (photons or particles) in a 2D plane in space by using an array of pixels which measure the waves independently.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#atomic_core_excitation -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#atomic_core_excitation">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
        <rdfs:comment>Detecting any of the results after atomic core excitation (fluorescence, photo-electrons or auger-electrons)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#auger_electrons -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#auger_electrons">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
        <rdfs:comment>Detecting one result after atomic core excitation (auger electrons)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#biological_sample -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#biological_sample">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_type"/>
        <rdfs:comment>Any material taken from a living organism or made of biological molecules, such as cells, tissues, membranes, proteins, nucleic acid, DNA, or viruses</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#technique_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result of which is being detected.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#charged_particles -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#charged_particles">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_input"/>
        <rdfs:comment>For example protons or helium ions</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#cryo -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#cryo">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_input"/>
        <rdfs:comment>Cryogenic temperature</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#technique_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result has properties which are being detected.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detected_photon_property -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detected_photon_property">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result being photons which properties are being measured.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#space_time"/>
        <rdfs:comment>Detect as a function of the incident photon or particle energy</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detection_resolution -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detection_resolution">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detected_cascade_of_events_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the resolution of the certain result properties are measured.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#detector_type -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#detector_type">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#technique_property"/>
        <rdfs:comment>The interaction between the sample and sample input(s) leads to a cascade of events, the result has properties which are being detected with a detector that has certain characteristics.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#diffraction -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#diffraction">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
        <rdfs:comment>Detecting interference of elastically scattering photons or particles</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#diffraction_of_elastic_scattering">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#diffraction"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#disperse_incident_energy_in_space -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#disperse_incident_energy_in_space">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_input_property"/>
        <rdfs:comment>Disperse incident wave photons in space with a polychromator (e.g. elliptically curved crystal in Bragg or Laue geometry).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#electrons -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#electrons">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_input"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#energy_dispersive_detector -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#energy_dispersive_detector">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detector_type"/>
        <rdfs:comment>Detector that distinguishes the energy of the waves (photons or particles) it measures with a semiconductor material which seperates energies based on the electrical current they generate in the semiconductor material.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#experimental_technique -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#experimental_technique">
        <rdfs:comment>A specific method or procedure used to perform experiments and gather data to investigate scientific questions.</rdfs:comment>
        <rdfs:comment>Every technique has a short description that tells what we measure and what you get out of it.

In addition, experimental techniques are semantically defined by
- *what* a technique measures
- *what* inputs it requires
- *how* the measurement</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#extended -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#extended">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detection_as_a_function_of_incident_energy"/>
        <rdfs:comment>Incident energy is scanned over a range &gt; 200 eV</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#fluorescence -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#fluorescence">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#cascade_of_events_being_detected"/>
        <rdfs:comment>Detecting one result after atomic core excitation (fluorescence)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#full-field_imaging -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#full-field_imaging">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#space_time"/>
        <rdfs:comment>Image of the sample in a single measurement (no motion of beam, sample or detector)</rdfs:comment>
        <rdfs:comment>Measure sample characteristics in a 2D plane in space by why of detection.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#hard_x-ray -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#hard_x-ray">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#named_photon_energy_ranges"/>
        <rdfs:comment>Defined using an object property (has_photon_energy_range) to connect to another class (photon_energy_range) that has the data properties (value and unit). It creates a chain where the characteristics of other classes included in the definition define the class. Works with the reasoner!</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#high_precision_in_Q_space -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#high_precision_in_Q_space">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detection_resolution"/>
        <rdfs:comment>For example min(ΔQ) = 1e-4 Å-1 to 5e-4 Å-1

Angular resolution: min(Δ2θ) = 0.001° to 0.005°

Resolving power (Δ2θ/θ)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#horizontal_linear_polarization -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#horizontal_linear_polarization">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#polarization"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#infrared -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#infrared">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#named_photon_energy_ranges"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#large_length_scales -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#large_length_scales">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#detection_resolution"/>
        <rdfs:comment>Length Scale d (Å) = 10 – 1000

Q-Range (Å−1) = 0.01 – 0.1

Large structural features: supramolecular structures, particle sizes, and shapes</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#laser -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#laser">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#sample_input"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#left_circular_polarization -->


    <owl:Class rdf:about="http://purl.org/pan-science/ESRFET#left_circular_polarization">
        <rdfs:subClassOf rdf:resource="http://purl.org/pan-science/ESRFET#polarization"/>
    </owl:Class>
    


    <!-- http://purl.org/pan-science/ESRFET#macro_sample_spatial_resolution -->


