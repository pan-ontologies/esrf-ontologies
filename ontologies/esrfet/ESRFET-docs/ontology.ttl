@prefix : <http://purl.org/pan-science/ESRFET#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/pan-science/ESRFET#> .

<http://purl.org/pan-science/ESRFET> rdf:type owl:Ontology ;
                                      owl:imports <http://purl.org/pan-science/PaNET/PaNET.owl> .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#altLabel
<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
<http://www.w3.org/2004/02/skos/core#prefLabel> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/pan-science/ESRFET#measures_as_a_function_of
:measures_as_a_function_of rdf:type owl:ObjectProperty ;
                           rdfs:domain :experimental_technique ;
                           rdfs:range :space_time ;
                           rdfs:comment """Example: XAS measures as function of incident X-ray photon energy
                X-ray absorption tomography measures as function of sample orientation
                Scanning Micro X-ray fluorescence measures as function of sample translation""" .


###  http://purl.org/pan-science/ESRFET#measures_interaction_result_property
:measures_interaction_result_property rdf:type owl:ObjectProperty ;
                                      rdfs:domain :experimental_technique ;
                                      rdfs:range :detected_cascade_of_events_property ;
                                      rdfs:comment "Example: XRF measures photon energy" .


###  http://purl.org/pan-science/ESRFET#measures_result_of_interaction
:measures_result_of_interaction rdf:type owl:ObjectProperty ;
                                rdfs:domain :experimental_technique ;
                                rdfs:range :cascade_of_events_being_detected ;
                                rdfs:comment "Example: XRF measures fluorscence" .


###  http://purl.org/pan-science/ESRFET#measures_transmission_with_detector_type
:measures_transmission_with_detector_type rdf:type owl:ObjectProperty ;
                                          rdfs:domain :transmission ;
                                          rdfs:range :detector_type .


###  http://purl.org/pan-science/ESRFET#measures_with_a_detector_type
:measures_with_a_detector_type rdf:type owl:ObjectProperty ;
                               rdfs:domain :experimental_technique ;
                               rdfs:range :detector_type .


###  http://purl.org/pan-science/ESRFET#requires_sample_input
:requires_sample_input rdf:type owl:ObjectProperty ;
                       rdfs:domain :experimental_technique ;
                       rdfs:range :sample_input ;
                       rdfs:comment "Example: Soft X-ray Absorption Spectroscopy uses soft X-ray as sample input" .


###  http://purl.org/pan-science/ESRFET#requires_sample_input_property
:requires_sample_input_property rdf:type owl:ObjectProperty ;
                                rdfs:domain :experimental_technique ;
                                rdfs:range :sample_input_property .


###  http://purl.org/pan-science/ESRFET#requires_sample_type
:requires_sample_type rdf:type owl:ObjectProperty ;
                      rdfs:domain :experimental_technique ;
                      rdfs:range :sample_type ;
                      rdfs:comment """Example: XRPD requires powder
                XRD does not require any sample type

                Another way of looking at it:
                diffraction (cascade of events being detected) requires 'any of' single crystal, powder, fiber
                x-ray powder diffraction (technique) requires 'exactly' powder""" .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/pan-science/ESRFET#atomic_distance_resolution_unit
:atomic_distance_resolution_unit rdf:type owl:DatatypeProperty .


###  http://purl.org/pan-science/ESRFET#atomic_distance_unit
:atomic_distance_unit rdf:type owl:DatatypeProperty ;
                      rdfs:domain :diffraction_of_elastic_scattering ;
                      rdfs:range xsd:string .


###  http://purl.org/pan-science/ESRFET#energy_range_unit
:energy_range_unit rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:string ;
                   rdfs:label "has energy range unit" .


###  http://purl.org/pan-science/ESRFET#energy_range_value
:energy_range_value rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:float ;
                    rdfs:label "has energy range value" .


###  http://purl.org/pan-science/ESRFET#sample_spatial_resolution
:sample_spatial_resolution rdf:type owl:DatatypeProperty .


###  http://purl.org/pan-science/ESRFET#sample_spatial_resolution_unit
:sample_spatial_resolution_unit rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.org/pan-science/ESRFET#ARPES
:ARPES rdf:type owl:Class ;
       rdfs:subClassOf :experimental_technique ;
       rdfs:comment "Angle-Resolved Photoemission Spectroscopy (ARPES) measures the energy and momentum of electrons emitted from a sample when illuminated by photons, mapping the electronic band structure and Fermi surface of materials." ;
       rdfs:label "ARPES" ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Angle-Resolved Photoemission Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#BCDI
:BCDI rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Bragg Coherent Diffraction Imaging (BCDI) measures the coherent diffraction around a Bragg peak from a crystalline sample to reconstruct the 3D shape and internal strain field of individual nanocrystals. The result is a high-resolution image of the crystalline structure and deformation." ;
      rdfs:label "BCDI" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Bragg Coherent Diffraction Imaging" .


###  http://purl.org/pan-science/ESRFET#BioSAXS
:BioSAXS rdf:type owl:Class ;
         rdfs:subClassOf :experimental_technique ;
         rdfs:comment "BioSAXS is SAXS applied to biological macromolecules in solution to determine their size, shape, flexibility, and aggregation state in near-native conditions." ;
         rdfs:label "BioSAXS" .


###  http://purl.org/pan-science/ESRFET#CDI
:CDI rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Coherent Diffraction Imaging (CDI) measures the far-field diffraction pattern from a sample illuminated with a coherent X-ray beam. The image of the sample is reconstructed computationally from the diffraction data using phase retrieval algorithms, yielding high-resolution, lensless imaging of the electron density or scattering potential." ;
     rdfs:label "CDI" ;
     <http://www.w3.org/2004/02/skos/core#altLabel> "CXD" ,
                                                    "CXDI" ,
                                                    "Coherent X-Ray Diffraction" ,
                                                    "Coherent X-ray Diffraction Imaging" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Coherent Diffraction Imaging" .


###  http://purl.org/pan-science/ESRFET#CMS
:CMS rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Conventional Mössbauer Spectroscopy measures the resonant absorption of X-rays by Mössbauer-active nuclei (e.g. Fe-57) using a radioactive source scanned in velocity. It provides information on oxidation state, magnetic ordering, and local electronic and structural environment through hyperfine interactions." ;
     rdfs:label "CMS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Conventional Mössbauer Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#CRYO-EM
:CRYO-EM rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_result_of_interaction ;
                                                      owl:someValuesFrom :transmission
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :requires_sample_input ;
                                                      owl:someValuesFrom :cryo
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :requires_sample_input ;
                                                      owl:someValuesFrom :electrons
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:comment "Cryogenic Electron Microscopy (Cryo-EM) is EM performed on rapidly frozen samples to preserve native structures in a near-physiological, vitrified state. It enables high-resolution 3D reconstruction of biological macromolecules and complexes without the need for crystallization." ;
         rdfs:label "CRYO-EM" ;
         <http://www.w3.org/2004/02/skos/core#altLabel> "Cryogenic electron microscopy" ;
         <http://www.w3.org/2004/02/skos/core#prefLabel> "Cryo-electron microscopy" .


###  http://purl.org/pan-science/ESRFET#CXDI
:CXDI rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Coherent X-ray Diffraction Imaging (CXDI) measures the coherent X-ray diffraction pattern from an isolated object or nanostructure. The 3D electron density is reconstructed computationally using phase retrieval, providing high-resolution, lensless imaging of the sample's internal structure." ;
      rdfs:label "CXDI" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Coherent X-ray Diffraction Imaging" .


###  http://purl.org/pan-science/ESRFET#DAC
:DAC rdf:type owl:Class ;
     rdfs:subClassOf :sample_input ;
     rdfs:comment "Diamond Anvil Cell to apply pressure on the sample" .


###  http://purl.org/pan-science/ESRFET#DAFS
:DAFS rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Diffraction Anomalous Fine Structure (DAFS) measures the intensity of Bragg diffraction as a function of X-ray photon energy near an absorption edge. It combines diffraction and XAS to provide element-specific structural information, such as site occupancy, bonding environment, and local distortions in crystals." ;
      rdfs:label "DAFS" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Diffraction Anomalous Fine Structure" .


###  http://purl.org/pan-science/ESRFET#DCT
:DCT rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_as_a_function_of ;
                                                  owl:someValuesFrom :tomography
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_result_of_interaction ;
                                                  owl:someValuesFrom :diffraction_of_elastic_scattering
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_result_of_interaction ;
                                                  owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :measures_transmission_with_detector_type ;
                                                                       owl:someValuesFrom :area_detector
                                                                     ]
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :requires_sample_input ;
                                                  owl:someValuesFrom :x-ray
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :requires_sample_type ;
                                                  owl:someValuesFrom :polycrystalline_sample
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:comment "DCT is a near-field diffraction-based imaging technique that provides high-resolution grain maps of polycrystalline materials (Ludwig et al. 2008). For each individual grain, the technique can provide orientation and average elastic strain tensor components with an accuracy of a few times 1e-4 (Reischig et al. 2013). The technique combines the concepts of image reconstruction from projections (tomography) and X-ray diffraction imaging (topography)." ;
     rdfs:label "DCT" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Diffraction Contrast Tomography" .


###  http://purl.org/pan-science/ESRFET#DFXM
:DFXM rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Dark-Field X-ray Microscopy (DFXM) measures X-ray diffraction in a dark-field imaging geometry to map crystalline structure, orientation, strain, and defects in 3D within bulk materials. It provides spatially resolved imaging of internal microstructures at micrometer to sub-micrometer resolution." ;
      rdfs:label "DFXM" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Dark-Field X-ray Microscopy" .


###  http://purl.org/pan-science/ESRFET#ED-EXAFS
:ED-EXAFS rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :measures_as_a_function_of ;
                                                       owl:someValuesFrom :extended
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :measures_interaction_result_property ;
                                                       owl:someValuesFrom :photon_count
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :measures_result_of_interaction ;
                                                       owl:someValuesFrom :atomic_core_excitation
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :requires_sample_input ;
                                                       owl:someValuesFrom :x-ray
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :requires_sample_input_property ;
                                                       owl:someValuesFrom :disperse_incident_energy_in_space
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:comment "Energy Dispersive Extended X-ray Absorption Fine Structure (ED-EXAFS) measures oscillations in X-ray absorption probability beyond the edge with energy-dispersive detection, providing local atomic structural information such as bond distances and coordination numbers, acquired quickly without angular scanning." ;
          rdfs:label "ED-EXAFS" ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive Extended X-ray Absorption Fine Structure" .


###  http://purl.org/pan-science/ESRFET#ED-XANES
:ED-XANES rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :measures_as_a_function_of ;
                                                       owl:someValuesFrom :near_edge
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :measures_interaction_result_property ;
                                                       owl:someValuesFrom :photon_count
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :measures_result_of_interaction ;
                                                       owl:someValuesFrom :atomic_core_excitation
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :requires_sample_input ;
                                                       owl:someValuesFrom :x-ray
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :requires_sample_input_property ;
                                                       owl:someValuesFrom :disperse_incident_energy_in_space
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:comment "Energy Dispersive X-ray Absorption Near Edge Structure (ED-XANES) measures the X-ray absorption probability near the absorption edge using energy-dispersive detection, providing rapid information about the oxidation state and electronic structure without scanning incident energy." ;
          rdfs:label "ED-XANES" ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive X-ray Absorption Near Edge Structure" .


###  http://purl.org/pan-science/ESRFET#ED-XAS
:ED-XAS rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_as_a_function_of ;
                                                     owl:someValuesFrom :detection_as_a_function_of_incident_energy
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_interaction_result_property ;
                                                     owl:someValuesFrom :photon_count
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_result_of_interaction ;
                                                     owl:someValuesFrom :atomic_core_excitation
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :requires_sample_input ;
                                                     owl:someValuesFrom :x-ray
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :requires_sample_input_property ;
                                                     owl:someValuesFrom :disperse_incident_energy_in_space
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:comment "Energy Dispersive X-ray Absorption Spectroscopy (ED-XAS) measures X-ray absorption as a function of energy simultaneously by dispersing the X-ray beam in space with a polychromatic source and position-sensitive detector, enabling rapid acquisition of absorption spectra without scanning the incident energy." ;
        rdfs:label "ED-XAS" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive X-ray Absorption Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#ED-XRD
:ED-XRD rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_interaction_result_property ;
                                                     owl:someValuesFrom :photon_energy
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_result_of_interaction ;
                                                     owl:someValuesFrom :diffraction_of_elastic_scattering
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_with_a_detector_type ;
                                                     owl:someValuesFrom :energy_dispersive_detector
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :requires_sample_input ;
                                                     owl:someValuesFrom :x-ray
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:comment "Energy Dispersive X-ray Diffraction (ED-XRD) measures diffraction patterns of a sample by detecting diffracted X-rays at a fixed angle while dispersing their energies, enabling fast phase identification and structural analysis without angular scanning." ;
        rdfs:label "ED-XRD" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive X-ray Diffraction" .


###  http://purl.org/pan-science/ESRFET#ED-XRF
:ED-XRF rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_interaction_result_property ;
                                                     owl:someValuesFrom :photon_count
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_interaction_result_property ;
                                                     owl:someValuesFrom :photon_energy
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_result_of_interaction ;
                                                     owl:someValuesFrom :fluorescence
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :measures_with_a_detector_type ;
                                                     owl:someValuesFrom :energy_dispersive_detector
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :requires_sample_input ;
                                                     owl:someValuesFrom :x-ray
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:comment "Energy Dispersive X-ray Fluorescence Spectroscopy (ED-XRF) measures the energies of fluorescent X-rays emitted by a sample excited by a broad-spectrum X-ray source, providing elemental composition analysis without scanning the incident energy." ;
        rdfs:label "ED-XRF" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive X-ray Fluorescence Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#ED-XRPD
:ED-XRPD rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_interaction_result_property ;
                                                      owl:someValuesFrom :photon_energy
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_result_of_interaction ;
                                                      owl:someValuesFrom :diffraction_of_elastic_scattering
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_with_a_detector_type ;
                                                      owl:someValuesFrom :energy_dispersive_detector
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :requires_sample_input ;
                                                      owl:someValuesFrom :x-ray
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :requires_sample_type ;
                                                      owl:someValuesFrom :powder_sample
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:comment "Energy Dispersive X-ray Powder Diffraction (ED-XRPD) measures diffraction patterns of powdered samples by detecting diffracted X-rays at a fixed angle while varying their energy, enabling rapid phase identification and structural analysis without scanning angles." ;
         rdfs:label "ED-XRPD" ;
         <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive X-ray Powder Diffraction" .


###  http://purl.org/pan-science/ESRFET#EDS
:EDS rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_interaction_result_property ;
                                                  owl:someValuesFrom :photon_count
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_interaction_result_property ;
                                                  owl:someValuesFrom :photon_energy
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_result_of_interaction ;
                                                  owl:someValuesFrom :fluorescence
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_with_a_detector_type ;
                                                  owl:someValuesFrom :energy_dispersive_detector
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :requires_sample_input ;
                                                  owl:someValuesFrom :electrons
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:comment "EDS measures X-ray photons coming from a sample during electron irradition  with an energy dispersive detector and derives elemental composition from the energy of the fluorescence photons" ;
     rdfs:label "EDS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Energy Dispersive X-ray Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#EM
:EM rdf:type owl:Class ;
    owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :requires_sample_input ;
                                                 owl:someValuesFrom :electrons
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
    rdfs:subClassOf :experimental_technique ;
    rdfs:comment "Electron Microscopy (EM) uses a beam of electrons as illumination, controlled by electron optics analogous to glass lenses in light microscopes, to measure electron scattering or transmission through a sample. This technique provides high-resolution images and electron diffraction patterns, revealing structural, morphological, and compositional information at nanometer to atomic scales." ;
    rdfs:label "EM" ;
    <http://www.w3.org/2004/02/skos/core#prefLabel> "Electron Microscopy" .


###  http://purl.org/pan-science/ESRFET#EXAFS
:EXAFS rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_as_a_function_of ;
                                                    owl:someValuesFrom :extended
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_interaction_result_property ;
                                                    owl:someValuesFrom :photon_count
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_result_of_interaction ;
                                                    owl:someValuesFrom :atomic_core_excitation
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :requires_sample_input ;
                                                    owl:someValuesFrom :x-ray
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:comment "Extended X-ray Absorption Fine Structure (EXAFS) measures oscillations in X-ray absorption above an element’s absorption edge, providing information on the local atomic environment, including distances, coordination numbers, and disorder around the absorbing atoms." ;
       rdfs:label "EXAFS" ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Extended X-ray Absorption Fine Structure" .


###  http://purl.org/pan-science/ESRFET#FF-EXAFS
:FF-EXAFS rdf:type owl:Class ;
          rdfs:subClassOf :experimental_technique ;
          rdfs:comment "FF-EXAFS measures the extended region of the absorption spectrum across a 2D sample area using broad X-ray beam and an area detector, providing spatially resolved information on local atomic structure, such as bond lengths and coordination." ;
          rdfs:label "FF-EXAFS" ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Full-Field Extended X-ray Absorption Fine Structure Imaging" .


###  http://purl.org/pan-science/ESRFET#FF-XANES
:FF-XANES rdf:type owl:Class ;
          rdfs:subClassOf :experimental_technique ;
          rdfs:comment "FF-XANES measures the near-edge region of the absorption spectrum across a 2D sample area using broad X-ray beam and an area detector, providing spatially resolved information on chemical state and electronic structure." ;
          rdfs:label "FF-XANES" ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Full‑Field X-ray Absorption Near Edge Structure Imaging" .


