     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Near-Field Ptychography (NFP) measures coherent diffraction patterns collected in the near-field regime by scanning overlapping illumination spots, enabling high-resolution phase and amplitude imaging of samples." ;
     rdfs:label "NFP" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Near-Field Ptychography" .


###  http://purl.org/pan-science/ESRFET#NFS
:NFS rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Nuclear Forward Scattering (NFS) measures the time-resolved resonant scattering of synchrotron-generated X-rays in the forward direction, extracting hyperfine parameters (e.g. isomer shift, quadrupole splitting, magnetic splitting) from the temporal interference pattern." ,
                  "Time-domain analog of Mössbauer spectroscopy" ;
     rdfs:label "NFS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Nuclear Forward Scattering" .


###  http://purl.org/pan-science/ESRFET#NR-XMLD
:NR-XMLD rdf:type owl:Class ;
         rdfs:subClassOf :experimental_technique ;
         rdfs:comment "Non-Reciprocal X-ray Magnetic Linear Dichroism (NR-XMLD) measures differences in linear X-ray absorption depending on magnetization direction and X-ray polarization, revealing magnetic asymmetries that break time-reversal symmetry." ;
         rdfs:label "NR-XMLD" ;
         <http://www.w3.org/2004/02/skos/core#prefLabel> "Non-Reciprocal X-ray Magnetic Linear Dichroism" .


###  http://purl.org/pan-science/ESRFET#NRIXS
:NRIXS rdf:type owl:Class ;
       rdfs:subClassOf :experimental_technique ;
       rdfs:comment "Nuclear Resonant Inelastic X-ray Scattering (NRIXS) measures the energy distribution of X-rays scattered inelastically by nuclear resonances, providing element-specific information on phonon density of states and lattice dynamics." ;
       rdfs:label "NRIXS" ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Nuclear Resonant Inelastic X-ray Scattering" .


###  http://purl.org/pan-science/ESRFET#NRS
:NRS rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Nuclear Resonant Scattering (NRS) measures time-delayed resonant scattering of X-rays from Mössbauer-active nuclei to study hyperfine interactions, magnetic order, and dynamics at the atomic scale." ;
     rdfs:label "NRS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Nuclear Resonant Scattering" .


###  http://purl.org/pan-science/ESRFET#PDF
:PDF rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Pair Distribution Function (PDF) analysis measures the distribution of interatomic distances by Fourier-transforming total scattering data, revealing local atomic arrangements and disorder in both crystalline and amorphous materials." ;
     rdfs:label "PDF" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Pair Distribution Function" .


###  http://purl.org/pan-science/ESRFET#PFY-XAS
:PFY-XAS rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_as_a_function_of ;
                                                      owl:someValuesFrom :detection_as_a_function_of_incident_energy
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_interaction_result_property ;
                                                      owl:someValuesFrom :photon_count
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_result_of_interaction ;
                                                      owl:someValuesFrom :atomic_core_excitation
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :measures_result_of_interaction ;
                                                      owl:someValuesFrom :fluorescence
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :requires_sample_input ;
                                                      owl:someValuesFrom :x-ray
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:comment "Partial Fluorescence Yield X-ray Absorption Spectroscopy (PFY-XAS) measures fluorescence photons from specific emission lines as a function of incident X-ray energy, providing element- and site-selective absorption spectra with reduced self-absorption effects." ;
         rdfs:label "PFY-XAS" ;
         <http://www.w3.org/2004/02/skos/core#prefLabel> "Partial Fluorescence Yield X-ray Absorption Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#PIXE
:PIXE rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_interaction_result_property ;
                                                   owl:someValuesFrom :photon_count
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_interaction_result_property ;
                                                   owl:someValuesFrom :photon_energy
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_result_of_interaction ;
                                                   owl:someValuesFrom :fluorescence
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :requires_sample_input ;
                                                   owl:someValuesFrom :charged_particles
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:comment "Particle Induced X-ray Emission (PIXE) measures characteristic X-rays emitted by a sample when irradiated with energetic charged particles (e.g., protons) to determine elemental composition with high sensitivity." ;
      rdfs:label "PIXE" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Particle Induced X-ray Emission" .


###  http://purl.org/pan-science/ESRFET#PTYCHO
:PTYCHO rdf:type owl:Class ;
        rdfs:subClassOf :experimental_technique ;
        rdfs:comment "Ptychography (PTYCHO) measures coherent diffraction patterns by scanning overlapping illumination spots on a sample in the far field, reconstructing high-resolution complex-valued images of sample structure beyond conventional lens limitations." ;
        rdfs:label "PTYCHO" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Ptychography" .


###  http://purl.org/pan-science/ESRFET#PXCT
:PXCT rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Ptychographic X-ray Computed Tomography (PXCT) combines ptychography and tomography to measure 3D electron density by scanning a coherent X-ray beam and recording diffraction patterns from overlapping regions at multiple angles. It provides high-resolution, quantitative 3D images of internal nanoscale structure." ;
      rdfs:label "PXCT" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Ptychographic X-ray Computed Tomography" .


###  http://purl.org/pan-science/ESRFET#REXS
:REXS rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Resonant Elastic X-ray Scattering (REXS) measures elastic scattering near an absorption edge to enhance contrast from specific elements, revealing electronic, magnetic, and orbital ordering in materials." ;
      rdfs:label "REXS" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Resonant Elastic X-ray Scattering" .


###  http://purl.org/pan-science/ESRFET#RIXS
:RIXS rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Resonant Inelastic X-ray Scattering (RIXS) measures energy loss of X-rays scattered resonantly from a sample, providing detailed insights into electronic excitations, spin dynamics, and valence states." ;
      rdfs:label "RIXS" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Resonant Inelastic X-ray Scattering" .


###  http://purl.org/pan-science/ESRFET#RXES
:RXES rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "Resonant X-ray Emission Spectroscopy (RXES) measures emitted X-rays following resonant excitation of core electrons, providing element- and valence-specific information about electronic structure and chemical states." ;
      rdfs:label "RXES" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Resonant X-ray Emission Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#S-XRD
:S-XRD rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_as_a_function_of ;
                                                    owl:someValuesFrom :scanning_imaging
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_result_of_interaction ;
                                                    owl:someValuesFrom :diffraction_of_elastic_scattering
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :requires_sample_input ;
                                                    owl:someValuesFrom :x-ray
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :experimental_technique ;
       rdfs:comment "Scanning X-ray Diffraction (S-XRD) measures diffraction patterns at many scanned positions on a sample using a focused X-ray beam, providing spatially resolved 2D maps of crystal structure, phase distribution, and strain within the sample." ;
       rdfs:label "S-XRD" ;
       <http://www.w3.org/2004/02/skos/core#altLabel> "SXDM" ,
                                                      "SXRD" ,
                                                      "Scanning X-ray Diffraction Microscopy" ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Scanning X-ray Diffraction" .


###  http://purl.org/pan-science/ESRFET#S-XRF
:S-XRF rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_as_a_function_of ;
                                                    owl:someValuesFrom :scanning_imaging
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_interaction_result_property ;
                                                    owl:someValuesFrom :photon_count
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_interaction_result_property ;
                                                    owl:someValuesFrom :photon_energy
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_result_of_interaction ;
                                                    owl:someValuesFrom :fluorescence
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :requires_sample_input ;
                                                    owl:someValuesFrom :x-ray
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:comment "Scanning X-ray Fluorescence (S-XRF) measures elemental composition by scanning a focused X-ray beam across a sample and detecting emitted fluorescence, producing high-resolution 2D maps of elemental distributions in the sample." ;
       rdfs:label "S-XRF" ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Scanning X-ray Fluorescence" .


###  http://purl.org/pan-science/ESRFET#SAD
:SAD rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "Single-wavelength Anomalous Diffraction (SAD) measures diffraction at one specific X-ray wavelength near an absorption edge to exploit anomalous scattering for phase determination in crystallography." ;
     rdfs:label "SAD" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Single-wavelength Anomalous Diffraction" .


###  http://purl.org/pan-science/ESRFET#SAXS
:SAXS rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_interaction_result_property ;
                                                   owl:someValuesFrom :large_length_scales
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_result_of_interaction ;
                                                   owl:someValuesFrom :diffraction_of_elastic_scattering
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :requires_sample_input ;
                                                   owl:someValuesFrom :x-ray
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:comment "Small-Angle X-ray Scattering (SAXS) measures the X-ray scattering intensity at small angles to obtain information about the size, shape, and structural organization of nanoscale features in a sample." ;
      rdfs:label "SAXS" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Small-Angle X-ray Scattering" .


###  http://purl.org/pan-science/ESRFET#SCXRD
:SCXRD rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_result_of_interaction ;
                                                    owl:someValuesFrom :diffraction_of_elastic_scattering
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :requires_sample_input ;
                                                    owl:someValuesFrom :x-ray
                                                  ]
                                                  [ rdf:type owl:Restriction ;
