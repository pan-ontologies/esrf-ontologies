                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_as_a_function_of ;
                                                    owl:someValuesFrom :near_edge
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_interaction_result_property ;
                                                    owl:someValuesFrom :photon_count
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :measures_result_of_interaction ;
                                                    owl:someValuesFrom :atomic_core_excitation
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :requires_sample_input ;
                                                    owl:someValuesFrom :x-ray
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:comment "XANES (X-ray Absorption Near Edge Structure) measures the fine structure in X-ray absorption near an element’s absorption edge, providing information about the oxidation state, electronic structure, and coordination geometry of the absorbing atoms." ;
       rdfs:label "XANES" ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Absorption Near Edge Structure" .


###  http://purl.org/pan-science/ESRFET#XAS
:XAS rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_as_a_function_of ;
                                                  owl:someValuesFrom :detection_as_a_function_of_incident_energy
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_interaction_result_property ;
                                                  owl:someValuesFrom :photon_count
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :measures_result_of_interaction ;
                                                  owl:someValuesFrom :atomic_core_excitation
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :requires_sample_input ;
                                                  owl:someValuesFrom :x-ray
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:comment "XAS measures a signal related to the X-ray absorption probability—such as fluorescence, photon electrons, Auger electrons, or transmission—as a function of the incident X-ray photon energy. It probes atomic core-level excitations by removing core-shell electrons, providing element-specific information about the local chemical environment, oxidation state, and atomic structure around the absorbing atoms." ;
     rdfs:label "XAS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Absorption Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#XBH
:XBH rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray Bragg Holography (XBH) measures interference patterns formed by Bragg-diffracted and reference X-rays from a crystal to reconstruct 3D atomic-scale images of the crystal structure." ;
     rdfs:label "XBH" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Bragg Holography" .


###  http://purl.org/pan-science/ESRFET#XCL
:XCL rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray Computed Laminography (XCL) measures X-ray transmission through flat, extended samples at multiple tilted angles to reconstruct high-resolution 3D images of internal structures that are challenging for conventional tomography." ;
     rdfs:label "XCL" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Computed Laminography" .


###  http://purl.org/pan-science/ESRFET#XDMR
:XDMR rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Detected Magnetic Resonance (XDMR) measures changes in X-ray absorption or emission induced by magnetic resonance excitation, providing element-specific information on spin dynamics and magnetic excitations." ;
      rdfs:label "XDMR" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Detected Magnetic Resonance" .


###  http://purl.org/pan-science/ESRFET#XDS
:XDS rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-Ray Diffuse Scattering (XDS) measures scattered X-rays away from Bragg peaks to probe local disorder, defects, thermal vibrations, and nanoscale structural fluctuations in materials." ;
     rdfs:label "XDS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-Ray Diffuse Scattering" .


###  http://purl.org/pan-science/ESRFET#XEOL
:XEOL rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Excited Optical Luminescence (XEOL) measures visible or near-visible light emitted from a sample after X-ray excitation, providing information on electronic structure, defect states, and luminescent centers." ;
      rdfs:label "XEOL" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Excited Optical Luminescence" .


###  http://purl.org/pan-science/ESRFET#XES
:XES rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray Emission Spectroscopy (XES) measures emitted X-ray photons from electronic transitions after X-ray excitation, revealing detailed information about the electronic structure and chemical state of elements. Unlike XRF, which identifies elemental composition through broad characteristic fluorescence lines, XES uses high energy resolution to resolve fine spectral features—such as valence-to-core transitions—providing deeper insight into the local chemical bonding and electronic environment." ;
     rdfs:label "XES" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Emission Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#XFCT
:XFCT rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :experimental_technique
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_as_a_function_of ;
                                                   owl:someValuesFrom :tomography
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_interaction_result_property ;
                                                   owl:someValuesFrom :photon_count
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_interaction_result_property ;
                                                   owl:someValuesFrom :photon_energy
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :measures_result_of_interaction ;
                                                   owl:someValuesFrom :fluorescence
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :requires_sample_input ;
                                                   owl:someValuesFrom :x-ray
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:comment "X-ray Fluorescence Tomography (XFCT) measures fluorescence X-rays emitted from a sample at multiple angles to reconstruct 3D elemental distributions with high sensitivity and spatial resolution." ;
      rdfs:label "XFCT" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Fluorescence Tomography" .


###  http://purl.org/pan-science/ESRFET#XFH
:XFH rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray Fluorescence Holography (XFH) measures interference between fluorescent X-rays emitted from atoms and scattered reference waves to obtain 3D atomic-scale images of the local atomic arrangement around specific elements." ;
     rdfs:label "XFH" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Fluorescence Holography" .


###  http://purl.org/pan-science/ESRFET#XHT
:XHT rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray HoloTomography (XHT) combines holographic imaging and tomography by recording holograms at multiple angles to reconstruct high-resolution 3D electron density maps of a sample." ;
     rdfs:label "XHT" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray HoloTomography" .


###  http://purl.org/pan-science/ESRFET#XLD
:XLD rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray Linear Dichroism (XLD) measures the difference in X-ray absorption for linearly polarized light oriented along different crystallographic axes, providing information on electronic anisotropy and orbital ordering." ;
     rdfs:label "XLD" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-Ray Linear Dichroism" .


###  http://purl.org/pan-science/ESRFET#XMCD
:XMCD rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Magnetic Circular Dichroism (XMCD) measures the difference in absorption of left- and right-circularly polarized X-rays in a magnetized sample, probing element-specific magnetic moments and spin states." ;
      rdfs:label "XMCD" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Magnetic Circular Dichroism" .


###  http://purl.org/pan-science/ESRFET#XMLD
:XMLD rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Magnetic Linear Dichroism (XMLD) measures differences in X-ray absorption of linearly polarized light depending on magnetic order, sensitive to antiferromagnetic and magnetic domain structures." ;
      rdfs:label "XMLD" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Magnetic Linear Dichroism" .


###  http://purl.org/pan-science/ESRFET#XMχD
:XMχD rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray MagnetoChiral Dichroism (XMχD) measures differences in X-ray absorption related to the combined effects of magnetism and chirality in materials, revealing chiral magnetic structures." ;
      rdfs:label "XMχD" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray MagnetoChiral Dichroism" .


###  http://purl.org/pan-science/ESRFET#XNCD
:XNCD rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Natural Circular Dichroism (XNCD) measures differences in absorption of circularly polarized X-rays in non-magnetic chiral materials, providing information on structural chirality and electronic asymmetry." ;
      rdfs:label "XNCD" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Natural Circular Dichroism" .


###  http://purl.org/pan-science/ESRFET#XNHT
:XNHT rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Nano-HoloTomography (XNHT) is a high-resolution variant of XHT, providing nanoscale 3D imaging of internal sample structure by combining holography and tomography with nanoscale focusing." ;
      rdfs:label "XNHT" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Nano-HoloTomography" .


###  http://purl.org/pan-science/ESRFET#XPCI
:XPCI rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Phase-Contrast Imaging (XPCI) measures phase shifts of X-rays transmitted through a sample to enhance image contrast, enabling visualization of structures with low absorption contrast such as soft tissues or light materials." ;
      rdfs:label "XPCI" ;
      <http://www.w3.org/2004/02/skos/core#altLabel> "PCXI" ,
                                                     "Phase-Contrast X-ray Imaging" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Phase-Contrast Imaging" .


###  http://purl.org/pan-science/ESRFET#XPCS
:XPCS rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-Ray Photon Correlation Spectroscopy (XPCS) measures time fluctuations of coherent X-ray scattering intensity to probe nanoscale dynamics and temporal correlations in materials such as particle motion, phase transitions, or relaxation processes." ;
      rdfs:label "XPCS" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "X-Ray Photon Correlation Spectroscopy" .


###  http://purl.org/pan-science/ESRFET#XPCT
:XPCT rdf:type owl:Class ;
      rdfs:subClassOf :experimental_technique ;
      rdfs:comment "X-ray Phase-Contrast Tomography (XPCT) measures phase shifts of X-rays passing through a sample at multiple angles to reconstruct 3D images with enhanced contrast for weakly absorbing materials, revealing internal structures not visible in conventional absorption tomography." ;
      rdfs:label "XPCT" ;
      <http://www.w3.org/2004/02/skos/core#prefLabel> "Phase-Contrast Tomography" .


###  http://purl.org/pan-science/ESRFET#XPS
:XPS rdf:type owl:Class ;
     rdfs:subClassOf :experimental_technique ;
     rdfs:comment "X-ray Photoelectron Spectroscopy (XPS) measures the kinetic energy of electrons ejected by X-rays to determine elemental composition and chemical states near the surface of a material." ;
     rdfs:label "XPS" ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "X-ray Photoelectron Spectroscopy" .


