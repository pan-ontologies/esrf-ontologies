        <AnnotationProperty IRI="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <IRI>#CDI</IRI>
        <Literal>Coherent X-ray Diffraction</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty IRI="http://www.w3.org/2004/02/skos/core#altLabel"/>
